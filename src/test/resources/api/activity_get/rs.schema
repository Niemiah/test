{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "activity": {
      "type": "string"
    },
    "accessibility": {
      "type": "number",
      "minimum": 0,
      "maximum": 1
    },
    "type": {
      "type": "string"
    },
    "participants": {
      "type": "integer",
      "minimum": 1
    },
    "price": {
      "type": "number",
      "minimum": 0
    },
    "link": {
      "type": "string",
      "format": "uri"
    },
    "key": {
      "type": "string"
    }
  }
}